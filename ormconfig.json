{
    "type": "postgres",
    "host": { "ENV": "DB_HOST" },
    "port": { "ENV": "DB_PORT" },
    "username": { "ENV": "DB_USER" },
    "password": { "ENV": "DB_PASSWORD" },
    "database": { "ENV": "DB_NAME" },
    "synchronize": true,
    "logging": false,
    "entities": [
      "src/entities/*.ts"
    ],
    "migrations": [
      "src/migrations/*.ts"
    ],
    "cli": {
      "entitiesDir": "src/entities",
      "migrationsDir": "src/migrations"
    }
  }
  